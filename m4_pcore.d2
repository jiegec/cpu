cpu : Apple M4 P-core CPU {
  frontend: Frontend {
    bp: Branch Predictor {
    }

    # Coupled Frontend
    l1ic: L1 IC {
      # Source: Geekerwan
      # Source: Apple
      # "192KiB, 6-way, 64B lines"
      l1ic: 192KB 6-way L1 IC

      # Source: Apple
      # "192 entries (3MiB)"
      l1itlb: 192-entry L1 ITLB
    }

    bp -> l1ic

    # Source: Geekerwan
    decode: 10-way Decode
    l1ic -> decode
    decode -> bp

    # Source: Geekerwan
    rename: 10-way Rename
    decode -> rename
  }

  backend: Backend {
    # Source: Geekerwan
    rob: 361-entry Coalesced ROB

    rf: Register File {
      # Source: Geekerwan
      irf: ~446-entry Integer Register File

      # Source: Geekerwan
      vrf: ~378-entry 128b Vector Register File
    }

    # Source: Geekerwan
    dispatch1: 15-entry Dispatch Queue \#1

    # Source: Geekerwan
    sched1: 87-entry Scheduler \#1

    # Source: Geekerwan
    pipe1: Pipe \#1 {
      ALU
      BR
      FLAGS
      ADR
      ALU/f + BRc/i in Apple's notation
    }
    dispatch1 -> sched1 -> rf.irf -> pipe1

    # Source: Geekerwan
    pipe2: Pipe \#2 {
      ALU
      BR
      FLAGS
      ADR
      ALU/f + BRc in Apple's notation
    }
    dispatch1 -> sched1 -> rf.irf -> pipe2

    # Source: Geekerwan
    pipe3: Pipe \#3 {
      ALU
      FLAGS
      ADR
      ALU/f in Apple's notation
    }
    dispatch1 -> sched1 -> rf.irf -> pipe3

    # Source: Geekerwan
    pipe4: Pipe \#4 {
      ALU
      FLAGS
      ADR
      ALU/f + PRED/f in Apple's notation
    }
    dispatch1 -> sched1 -> rf.irf -> pipe4

    # Source: Geekerwan
    dispatch2: 15-entry Dispatch Queue \#2

    # Source: Geekerwan
    sched2: 19-entry Scheduler \#2

    # Source: Geekerwan
    pipe5: Pipe \#5 {
      ALU
      ADR
      MUL
      ALU + MUL in Apple's notation
    }
    dispatch2 -> sched2 -> rf.irf -> pipe5

    # Source: Geekerwan
    sched3: 29-entry Scheduler \#3

    # Source: Geekerwan
    pipe6: Pipe \#6 {
      ALU
      MUL
      ALU + MUL in Apple's notation
    }
    dispatch2 -> sched3 -> rf.irf -> pipe6

    # Source: Geekerwan
    sched4: 17-entry Scheduler \#4

    # Source: Geekerwan
    pipe7: Pipe \#7 {
      ALU
      DIV
      ALU + DIV in Apple's notation
    }
    dispatch2 -> sched4 -> rf.irf -> pipe7

    # Source: Geekerwan
    sched5: 23-entry Scheduler \#5

    # Source: Geekerwan
    pipe8: Pipe \#8 {
      ALU
      MUL
      BFM
      ALU + MUL + MISC in Apple's notation
    }
    dispatch2 -> sched5 -> rf.irf -> pipe8

    # Source: Geekerwan
    dispatch3: 15-entry Dispatch Queue \#3

    # Source: Geekerwan
    sched6: 72-entry Scheduler \#6

    # Source: Geekerwan
    pipe9: Pipe \#9 {
      STORE
    }
    dispatch3 -> sched6 -> rf.irf -> pipe9

    # Source: Geekerwan
    pipe10: Pipe \#10 {
      LOAD
      STORE
    }
    dispatch3 -> sched6 -> rf.irf -> pipe10

    # Source: Geekerwan
    pipe11: Pipe \#11 {
      LOAD
    }
    dispatch3 -> sched6 -> rf.irf -> pipe11

    # Source: Geekerwan
    pipe12: Pipe \#12 {
      LOAD
    }
    dispatch3 -> sched6 -> rf.irf -> pipe12

    lsu: LSU {
      # Source: Geekerwan
      131-entry Load Queue
      72-entry Store Queue
    }

    pipe9 -> lsu
    pipe10 -> lsu
    pipe11 -> lsu
    pipe12 -> lsu

    rob -> dispatch1
    rob -> dispatch2
    rob -> dispatch3

    # Source: Geekerwan
    dispatch4: 23-entry Dispatch Queue \#4

    # Source: Geekerwan
    sched7: 61-entry Scheduler \#7

    # Source: Geekerwan
    pipe13: Pipe \#13 {
      FP
      SIMD
    }
    dispatch4 -> sched7 -> rf.vrf -> pipe13

    # Source: Geekerwan
    sched8: 61-entry Scheduler \#8

    # Source: Geekerwan
    pipe14: Pipe \#14 {
      FP
      SIMD
    }
    dispatch4 -> sched8 -> rf.vrf -> pipe14

    # Source: Geekerwan
    sched9: 61-entry Scheduler \#9

    # Source: Geekerwan
    pipe15: Pipe \#15 {
      FP
      SIMD
      FCSEL
      TO INT
    }
    dispatch4 -> sched9 -> rf.vrf -> pipe15

    # Source: Geekerwan
    sched10: 61-entry Scheduler \#10

    # Source: Geekerwan
    pipe16: Pipe \#16 {
      FP
      SIMD
      DIV
      FSQRT
      FCSEL
      TO INT
    }
    dispatch4 -> sched10 -> rf.vrf -> pipe16
    rob -> dispatch4
  }
  frontend.rename -> backend.rob

  mem: Memory {
    l1: L1 DC {
      # Source: Geekerwan
      # Source: Apple
      # "128KiB, 8-way, 64B lines"
      l1dc: 128KB 8-way L1DC

      # Source: Apple
      # "160 entries (2.5MiB)"
      l1dtlb: 160-entry L1 DTLB
    }

    # Source: Apple
    # "3072 entries (48MiB)"
    l2tlb: 3072-entry L2 TLB

    l2: L2 {
      # Source: Apple
      # "16MiB, 16-way, 128B lines"
      l2dc: 16MB 16-way L2 Cache
    }
    l1 -> l2
  }
  backend.lsu -> mem.l1

  info: |md
    Drawn by Jiajie Chen @jiegec

    Based on data from Geekerwan
  |
}